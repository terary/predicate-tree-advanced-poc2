  },
  "scripts": {
    "test": "jest",
    "test:verbose": "npm run test -- --watchAll=false --verbose",
    "test:coverage": "npm run test -- --watchAll=false --verbose --coverage  --collectCoverageFrom=src/**/*.ts* ",
