    "@types/lodash.pick": "^4.4.6",
    "@types/node": "^16.4.8",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-jest": "^27.0.6",
    "commitizen": "^4.2.4",
