{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "eslint.alwaysShowStatus": true,
  "cSpell.words": [
    "betty",
    "betweeni",
    "betweenx",
    "birthdate",
    "clonedeep",
    "consts",
    "datetime",
    "differencewith",
    "firstname",
    "flintstone",
    "Flintstone",
    "fred",
    "lastname",
    "nany",
    "obfus",
    "Projectable",
    "projectables",
    "subtreeroot",
    "testables",
    "uuidv4",
    "wilma"
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true
  },
  "jest.jestCommandLine": "npm test --"
}
